<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhongshan.mapper.CheckStandardMapper">

    <resultMap id="BaseResultMap" type="com.zhongshan.entity.CheckStandard">
            <id property="checkNo" column="check_no" jdbcType="VARCHAR"/>
            <result property="checkName" column="check_name" jdbcType="VARCHAR"/>
            <result property="checkPay" column="check_pay" jdbcType="DOUBLE"/>
            <result property="checkUnit" column="check_unit" jdbcType="VARCHAR"/>
            <result property="checkType" column="check_type" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        check_no,check_name,check_pay,
        check_unit,check_type
    </sql>
</mapper>
